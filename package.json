{
  "name": "@nkorai/json-xform",
  "version": "1.3.4-nk.0",
  "description": "A small library to perform transformations between JSON data.",
  "homepage": "https://github.com/perpk/json-xform#readme",
  "bugs": {
    "url": "https://github.com/perpk/json-xform/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perpk/json-xform.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"CommonJS\"}' mocha -r ts-node/register test/**/*.test.ts",
    "test:reportJson": "nyc mocha --reporter json --reporter-option output=mocha-report.json",
    "test:report": "npx mocha --reporter mochawesome",
    "test:watch": "mocha --watch",
    "coverage:report": "npx nyc report --reporter json-summary",
    "lint": "npx eslint . --ext .js,.ts",
    "lint:fix": "npx eslint . --ext .js,.ts --fix",
    "validate": "npm-run-all --parallel lint test",
    "prepare": "husky install",
    "precommit": "npm run validate",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "async": "^3.2.6",
    "date-fns": "^4.1.0",
    "jsonpath": "^1.1.1",
    "jsonpath-plus": "^10.3.0",
    "jsonschema": "^1.5.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-chai-friendly": "^1.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rewire": "^5.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.100.0",
    "webpack-cli": "^6.0.1"
  }
}
